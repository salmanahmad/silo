
function(
    name(add)
    inputs(a : int, b : int)
    outputs(int)
    {
        a + b
    }
)

function(
    name(bar)
    outputs(int)
    {
        42
    }
)

function({
    5 | add(3)
})

function({
    bar() | add(-2)
})

function({
    i : int = bar() | add(8)
    return(i)
})

